<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<ui:style>
		.globalStyle {
			background: #000000 repeat scroll 0 0;
			background-color: black;
			color: white;
		}
		.globalStyle2 {
			background: #000000 repeat scroll 0 0;
			background-color: black;
			width: 100%;
			height: 100%;
		}
		.autoflow {
			overflow-y: auto;
		}
		.zindex {
			z-index: 42;
		}
	</ui:style>

	<g:PopupPanel modal="true" autoHideEnabled="true" glassEnabled="true" addStyleNames="{style.zindex}">
		<g:HTMLPanel addStyleNames="{style.globalStyle}" ui:field="pane" />
		<!-- Patate -->
		<!-- <g:Image url="Documents/Exposition.jpg" ui:field="expositionImage" altText="Exposition" /> -->
	</g:PopupPanel>
</ui:UiBinder>
